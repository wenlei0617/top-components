ypes.oneOf([\"small\", \"large\", \"default\"]),\n    value: Proptypes.string,\n    type: Proptypes.string,\n    onChange: Proptypes.func,\n    style: Proptypes.object,\n    placeholder: Proptypes.string\n  };\n  public static defaultProps = {\n    defaultValue: \"\",\n    disabled: false,\n    size: \"default\",\n    type: \"text\",\n    placeholder: \"\"\n  };\n\n  public render() {\n    const {\n      disabled,\n      id,\n      type,\n      size,\n      style,\n      placeholder,\n      value,\n      onChange,\n      defaultValue,\n      ...rest\n    } = this.props;\n    const className = classnames({\n      \"topC-input\": true,\n      [`topC-input-${size}`]: !!size\n    });\n    const meProps = defaultValue\n      ? {\n          defaultValue: defaultValue\n        }\n      : {\n          onChange: onChange,\n          value: value\n        };\n    return (\n      <input\n        style={style}\n        className={className}\n        id={id}\n        placeholder={placeholder}\n        type={type}\n        data-size={size}\n        disabled={disabled}\n        {...meProps}\n        {...rest}\n      />\n    );\n  }\n}\nexport default Input;\n"]}]}