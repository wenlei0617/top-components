C,CAVF,EAWGT,YAAY,CAAC;AACZC,IAAAA,cAAc,EAAdA,cADY;AAEZC,IAAAA,UAAU,EAAVA,UAFY;AAGZC,IAAAA,MAAM,EAANA,MAHY;AAIZC,IAAAA,MAAM,EAANA,MAJY;AAKZC,IAAAA,MAAM,EAANA,MALY;AAMZN,IAAAA,QAAQ,EAARA,QANY;AAOZO,IAAAA,IAAI,EAAJA;AAPY,GAAD,CAXf,CADF,CADF,CADF;AA2BD,CA1CD;;AA2CAC,KAAK,CAACG,SAAN,GAAkB;AAChBC,EAAAA,KAAK,EAAErB,SAAS,CAACsB,MADD;AAEhBC,EAAAA,MAAM,EAAEvB,SAAS,CAACsB,MAFF;AAGhBd,EAAAA,QAAQ,EAAER,SAAS,CAACwB,IAHJ;AAIhBb,EAAAA,cAAc,EAAEX,SAAS,CAACwB,IAJV;AAKhBZ,EAAAA,UAAU,EAAEZ,SAAS,CAACyB,IALN;AAMhBZ,EAAAA,MAAM,EAAEb,SAAS,CAACyB,IANF;AAOhBX,EAAAA,MAAM,EAAEd,SAAS,CAAC0B,MAPF;AAQhBnB,EAAAA,KAAK,EAAEP,SAAS,CAACyB,IARD;AAShBP,EAAAA,OAAO,EAAElB,SAAS,CAACwB;AATH,CAAlB;AAWAP,KAAK,CAACU,YAAN,GAAqB;AACnBnB,EAAAA,QAAQ,EAAE,IADS;AAEnBG,EAAAA,cAAc,EAAE,KAFG;AAGnBC,EAAAA,UAAU,EAAE,KAHO;AAInBC,EAAAA,MAAM,EAAE,KAJW;AAKnBC,EAAAA,MAAM,EAAE,SALW;AAMnBP,EAAAA,KAAK,EAAE,IANY;AAOnBW,EAAAA,OAAO,EAAE;AAPU,CAArB;eASeD,K;AAAf;;;;;;;;;;;;;;;;;;;;;;0BA/FMb,Y;0BAgBAM,Y;0BAgBAO,K","sourcesContent":["import * as React from \"react\";\nimport CreateModal from \"./createModel\";\nimport PropTypes from \"prop-types\";\nimport classnames from \"classnames\";\nimport Button from \"../Button\";\nimport Icon from \"../Icon\";\nimport \"./styles/model.scss\";\nimport { buttonType } from \"../Button/button\";\n\nexport interface IheadProps {\n  title?: string;\n  header?: React.ReactNode;\n  closable?: boolean; // 是否显示右上角的关闭按钮\n  onCancel?: () => void; // 取消回调\n}\nexport interface IfooterProps {\n  footer?: React.ReactNode;\n  cancelText?: React.ReactNode;\n  okText?: React.ReactNode;\n  okType?: buttonType;\n  confirmLoading?: boolean; // 确认按钮 loading\n  onOk?: () => void; // 点击确定回调\n  onCancel?: () => void; // 取消回调\n}\n\nexport interface Iprops extends IheadProps, IfooterProps {\n  width?: boolean;\n  visible?: boolean;\n  zIndex?: number;\n  children: React.ReactNode;\n}\nconst renderHeader = (props: IheadProps) => {\n  const { header, title, closable, onCancel } = props;\n  if (header) {\n    return header;\n  }\n  return (\n    <div className=\"topC-modal-header\">\n      <p>{title}</p>\n      {closable && (\n        <span onClick={onCancel}>\n          <Icon name=\"close\" />\n        </span>\n      )}\n    </div>\n  );\n};\nconst renderFooter = (props: IfooterProps) => {\n  const { confirmLoading, cancelText, okText, okType, footer, onOk, onCancel } = props;\n  if (footer) {\n    return <div className=\"topC-modal-footer\">{footer}</div>;\n  }\n\n  return (\n    <div className=\"topC-modal-footer\">\n      <Button onClick={onCancel}>{cancelText}</Button>\n      <Button type={okType} loading={confirmLoading} htmlType=\"submit\" onClick={onOk}>\n        {okText}\n      </Button>\n    </div>\n  );\n};\n\nconst Modal = (props: Iprops) => {\n  const {\n    visible,\n    closable,\n    title = \"\",\n    header = null,\n    confirmLoading,\n    cancelText,\n    okText,\n    okType,\n    footer,\n    children,\n    onCancel,\n    onOk\n  } = props;\n  return (\n    <CreateModal visible={visible}>\n      <div className=\"topC-modal-warpper\">\n        <div\n          className={classnames(\"topC-modal\", {\n            \"topC-modal-center\": closable\n          })}>\n          {renderHeader({\n            title,\n            header,\n            closable,\n            onCancel\n          })}\n          <div className=\"topC-modal-body\">{children}</div>\n          {renderFooter({\n            confirmLoading,\n            cancelText,\n            okText,\n            okType,\n            footer,\n            onCancel,\n            onOk\n          })}\n        </div>\n      </div>\n    </CreateModal>\n  );\n};\nModal.propTypes = {\n  width: PropTypes.number,\n  zIndex: PropTypes.number,\n  closable: PropTypes.bool,\n  confirmLoading: PropTypes.bool,\n  cancelText: PropTypes.node,\n  okText: PropTypes.node,\n  okType: PropTypes.string,\n  title: PropTypes.node,\n  visible: PropTypes.bool\n};\nModal.defaultProps = {\n  closable: true,\n  confirmLoading: false,\n  cancelText: \"取 消\",\n  okText: \"确 定\",\n  okType: \"primary\",\n  title: \"标题\",\n  visible: false\n};\nexport default Modal;\n"]}]}